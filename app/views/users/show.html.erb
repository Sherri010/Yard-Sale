 <h4>Hi <%= @user.first_name  %>!</h4>
 <div class="row">

      <div id="info" class=" col s3  blue-grey darken-4">
        <!-- Grey navigation panel -->
        <ul>
           <li><b> <%= @user.first_name %> <%= @user.last_name %></b></li>
           <li> <%= @user.email %></li>
           <li>Address:  <%= @user.address %></li>
           <li> <%= @user.city %></li>
           <li><%= @user.state %> </li>
        </ul>
        <a class="waves-effect waves-teal btn-flat">Edit</a>
      </div>

      <div id="history" class=" col s9 blue-grey lighten-3">
        <!-- Teal page content  -->
        <div id="owns">
          <h5> Your items for sale: </h5>
           <table>
            <tbody>
              <% @owns.each do |i| %>
              <tr>
                <td> <%= i.name %> </td>
                <td> $<%= i.price %> </td>
                <td><% if i.available %><b>not sold</b><%else%> sold<% end %></td>
              </tr>
               <% end %>
            </tbody>
          </table>
        </div>
      </div>

    </div>
